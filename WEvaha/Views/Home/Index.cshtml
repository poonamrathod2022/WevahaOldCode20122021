<script type="text/javascript">
    $(document).ready(function () {
           $('#btnSubmit').click(function () {
            $("#RegisterForm").validate({
                rules: {
                    email: {
                        required: true,
                        email: true
                    },

                    password: {
                        required: true,
                        minlength: 6
                    },

                    cnfpassword: {
                        required: true,
                        equalTo: "#password"
                    }

                },
                messages: {

                    email: {
                        required: "Enter Email",
                        email: "Please Enter Valid Email"
                    },
                    password: {
                        required: "Enter password",
                        minlength: "Password must be at least 6 characters long"
                    },
                    cnfpassword: {
                        required: "Enter confirm password",
                        equalTo: "Password and Confirm password Miss-match"
                    }
                },
                submitHandler: function (form) {
                        var email = $("#email").val();
                        var password = $("#password").val();
                        var Gtype = $("input:radio[name='Gender']:checked").val();
                    var SGtype = $("input:radio[name='SeekingGender']:checked").val();
                    var location = $("#hdLocation").val();

                    $.ajax({
                        url: "@Url.Action("SaveRegister", "Home")",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        data: { email: email, Gender: Gtype, SeekingGender: SGtype, password: password, Location: location},  // data to submit
                        success: function (data, status, xhr) {
                            if (data == -2) {
                                swal({
                                    icon: "warning",
                                    text: "Given Email is already Registered with us",
                                    type: "success",
                                    buttons: false
                                });
                                window.setTimeout(function () { window.location = "../Home/Index"; }, 3000);
                            }
                            else {
                                $("#txtEmailid").val(email);
                                $("#hdnProfileId").val(data);
                                $('#myModal').modal('show');
                                $("#email").val('');
                                $("#password").val('');
                                $("#cnfpassword").val('');
                            }
                        },
                        error: function (jqXhr, textStatus, errorMessage) {
                            swal({
                                icon: "warning",
                                text: "Something Could be wrong",
                                type: "success",
                                buttons: false
                            });
                            window.setTimeout(function () { window.location = "../Home/Index"; }, 3000);
                        }
                    });

                }
            });
        });

    });
</script>

